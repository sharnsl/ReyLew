# Combining Datasets with Outer join
DeathUS <- merge(x = FPGuns, y = opioid, by = "...1", all = TRUE)
View(DeathUS)

#rename columns
names(DeathUS)[names(DeathUS) == "sex"] <- "gSex"
names(DeathUS)[names(DeathUS) == "age"] <- "gAge"
names(DeathUS)[names(DeathUS) == "race"] <- "gRace"
names(DeathUS)[names(DeathUS) == "Sex"] <- "oSex"
names(DeathUS)[names(DeathUS) == "Age"] <- "oAge"
names(DeathUS)[names(DeathUS) == "Race"] <- "oRace"
